{
  "manifest_version": 3,
  "name": "Video Parser Extension",
  "version": "1.0",
  "description": "解析影视链接并跳转到解析后的URL",
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
